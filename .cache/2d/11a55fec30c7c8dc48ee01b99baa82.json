olor: #ebe3d8;\r\n  --theme1-btn-text-color: #66739a;\r\n  --theme1-btn-accent-color: #d8402b;\r\n  --theme1-box-color: #252d44;\r\n\r\n  --theme2-bg-color: #e6e6e6;\r\n  --theme2-btn-color: #e5e4e1;\r\n  --theme2-btn-text-color: #388187;\r\n  --theme2-btn-accent-color: #c05202;\r\n  --theme2-box-color: #d3cdcf;\r\n\r\n  --theme3-bg-color: #17062a;\r\n  --theme3-btn-color: #341a4e;\r\n  --theme3-btn-text-color: #56077b;\r\n  --theme3-btn-accent-color: #00dece;\r\n  --theme3-font-color: #fce44b;\r\n  --theme3-box-color: #1e0836;\r\n}\r\n\r\nbody {\r\n  height: 98vh;\r\n  font-family: \"Poppins\", sans-serif;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n[type=\"button\"],\r\n[type=\"reset\"],\r\n[type=\"submit\"] {\r\n  background-color: transparent;\r\n  border-width: 0;\r\n  padding: 0;\r\n  cursor: pointer;\r\n}\r\n\r\n/* Theme color */\r\ndiv.calc-container[data-theme=\"1\"] {\r\n  background-color: var(--theme1-bg-color);\r\n}\r\n\r\ndiv.calc-container[data-theme=\"2\"] {\r\n  background-color: var(--theme2-bg-color);\r\n}\r\n\r\ndiv.calc-container[data-theme=\"3\"] {\r\n  background-color: var(--theme3-bg-color);\r\n}\r\n\r\ndiv.theme-options[data-theme=\"1\"],\r\ndiv.calc-keyboard[data-theme=\"1\"],\r\ndiv.calc-answer-box[data-theme=\"1\"] {\r\n  background-color: var(--theme1-box-color);\r\n}\r\n\r\ndiv.theme-options[data-theme=\"2\"],\r\ndiv.calc-keyboard[data-theme=\"2\"],\r\ndiv.calc-answer-box[data-theme=\"2\"] {\r\n  background-color: var(--theme2-box-color);\r\n}\r\n\r\ndiv.theme-options[data-theme=\"3\"],\r\ndiv.calc-keyboard[data-theme=\"3\"],\r\ndiv.calc-answer-box[data-theme=\"3\"] {\r\n  background-color: var(--theme3-box-color);\r\n}\r\n\r\ninput.theme-option.checked[data-theme=\"1\"],\r\ninput#equal[data-theme=\"1\"] {\r\n  background-color: var(--theme1-btn-accent-color);\r\n  color: white;\r\n}\r\n\r\ninput.theme-option.checked[data-theme=\"2\"],\r\ninput#equal[data-theme=\"2\"] {\r\n  background-color: